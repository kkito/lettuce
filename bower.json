{
  "name": "lettuce",
  "version": "0.0.0",
  "homepage": "https://github.com/kkito/lettuce",
  "authors": [
    "kk.shen <kk.shen@saybot.com>"
  ],
  "description": "lettuce's bower",
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "~2.1.3",
    "underscore": "~1.7.0",
    "react": "~0.12.2",
    "highlight": "~7.0.1"
  },
  "overrides": {
    "jquery": {
      "main": "dist/jquery.min.js"
    },
    "underscore": {
      "main": "underscore-min.js"
    },
    "react": {
      "main": [
        "react.min.js",
        "JSXTransformer.js"
      ]
    },
    "highlight":{
        "main": [
            "src/highlight.js",
            "src/languages/*",
            "src/styles/*"
        ]
    }
  }

}
